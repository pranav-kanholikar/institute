<p id="notice"><%= notice %></p>

puts params.inspect

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>S name</th>
      <th>Roll no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.s_name %></td>
        <td><%= student.roll_no %></td>
        <td><%= link_to 'Show', "" %></td>
        <td><%= link_to 'Edit', "" %></td>
        <td><%= link_to 'Destroy', "", method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', "" %>
